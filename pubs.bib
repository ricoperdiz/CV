@MastersThesis{Perdiz2011,
  author    = {Perdiz, R. O.},
  title     = {Sapindaceae em remanescentes de floresta montana no sul da {Bahia}, {Brasil}},
  school    = {Universidade Estadual de Feira de Santana},
  year      = {2011},
  type      = {Master's thesis},
  abstract  = {Este trabalho apresenta um tratamento taxonômico de Sapindaceae em remanescentes de florestas montanas do sul da Bahia, Brasil. O objetivo principal foi realizar o levantamento de Sapindaceae em três remanescentes de floresta montana na região sul do estado da Bahia. O primeiro capítulo apresenta uma visão geral da morfologia do grupo, baseada nas espécies amostradas no estudo florístico. O segundo capítulo trata da descrição de uma nova espécie (Paullinia unifoliolata) encontrada neste inventário. O terceiro capítulo apresenta o resultado do levantamento florístico. Foram encontradas 21 espécies em seis gêneros. Paullinia apresentou a maior riqueza de espécies (7 spp.), seguido de Cupania (5 spp.), Allophylus (4 spp.), Matayba e Serjania (2 spp. cada) e Talisia (1 sp.). Allophylus leucoclados ̧ Cupania furfuracea, Matayba intermedia e Paullinia micrantha representam novos registros para a região Nordeste. Paullinia unifoliolata, espécie recém-descrita a partir deste estudo, é conhecida de uma região entre Arataca e Una. Apenas um espécime não foi determinado especificamente, por não se enquadrar em nenhuma das espécies descritas do gênero (Cupania sp. 1). Todas as espécies de Cupania e cinco de Paullinia são endêmicas do domínio Atlântico. As áreas amostradas neste estudo apresentaram a maior diversidade de espécies de Sapindaceae para o domínio Atlântico, em comparação com outros estudos no mesmo domínio. São apresentadas chave de identificação das espécies, descrições, e informações sobre floração, frutificação, distribuição geográfica e hábitat.},
  file      = {:/Users/ricoperdiz/Documents/producao_cientifica/teses/2011_Perdiz-mestrado.pdf:PDF},
  owner     = {ricoperdiz},
  timestamp = {2019.01.14},
}

